<view class="loginPage">
            <van-cell-group>
            <van-field value="{{ phonenumber }}"  required clearable label="手机号" placeholder="请输入手机号" bind:input="usernameHandle" />
            <van-field value="{{ password }}" type="password" clearable label="密码" placeholder="请输入密码" required bind:input="pwdHandle" />
        </van-cell-group>
        <view class="footer">
            <navigator style="font-size:25rpx;padding-bottom:20rpx;" url="../sigin/sigin" >没有账号？点击注册</navigator>
            <van-button type="primary" round size="large" bindtap = "loginHandle">登陆</van-button>
        </view>
</view>