<view class="collect" wx:if="{{collectNum}}">
    <block class="collections" wx:for="{{}}">
        <navigator class="item" url="../details/details">
            <view class="msgtop">
                <text class="jobname" decode="{{true}}">{{item.jobname}}</text>
                <text class="salary" decode="{{true}}">{{item.salary}}</text>
            </view>
            <view class="msgbtm">
                <text class="address" decode="{{true}}">{{item.address}}</text>
                <text class="date">{{item.date}}</text>
            </view>
            <text class="company">{{item.address}}</text>
        </navigator>
    </block>
</view>


<view class="nothing" wx:if="{{collectNum==0}}">
   <view class="content">
      <image src="../../assets/img/icon-workmate.png"></image><text>暂无记录哦</text>
   </view>

</view>

